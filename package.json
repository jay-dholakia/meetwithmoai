{
  "name": "moai-friends",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "supabase:deploy": "./scripts/deploy.sh",
    "supabase:dev": "./scripts/dev.sh",
    "supabase:types": "supabase gen types typescript --project-id hgllvhohhyamsbljekrd --schema public > lib/database.types.cli.ts",
    "supabase:status": "supabase projects list | grep hgllvhohhyamsbljekrd",
    "deno:info": "deno run --allow-run --allow-read --allow-write scripts/supabase-deno.ts info",
    "deno:status": "deno run --allow-run --allow-read --allow-write scripts/supabase-deno.ts status",
    "deno:deploy": "deno run --allow-run --allow-read --allow-write scripts/supabase-deno.ts deploy",
    "deno:types": "deno run --allow-run --allow-read --allow-write scripts/supabase-deno.ts types",
    "deno:edge": "deno run --allow-run --allow-read --allow-write scripts/supabase-deno.ts edge",
    "openai:setup": "./scripts/setup-openai.sh",
    "migrate:intake": "npx ts-node scripts/migrate-intake-data.ts"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@supabase/supabase-js": "^2.56.0",
    "dotenv": "^17.2.1",
    "expo": "~53.0.20",
    "expo-status-bar": "~2.2.3",
    "expo-location": "~17.0.1",
    "expo-av": "~14.0.1",
    "expo-image-picker": "~15.0.1",
    "expo-linear-gradient": "~13.0.1",
    "expo-haptics": "~13.0.1",
    "openai": "^5.15.0",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-screens": "~4.0.0",
    "react-native-safe-area-context": "4.10.5",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.12.0",
    "expo-dev-client": "~5.2.4",
    "react-dom": "19.0.0",
    "react-native-web": "^0.20.0",
    "@expo/metro-runtime": "~5.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "typescript": "~5.8.3"
  },
